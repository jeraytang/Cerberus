@model IdentityServer4.SecurityTokenService.Models.Account.RedirectViewModel

<h1>You are now being returned to the application.</h1>
<p>Once complete, you may close this tab</p>

<meta http-equiv="refresh" content="0;url=@Model.RedirectUrl" data-url="@Model.RedirectUrl">
<script type="text/javascript">
$(function() {window.location.href = document.querySelector("meta[http-equiv=refresh]").getAttribute("data-url"); })
</script>
