@model IdentityServer4.SecurityTokenService.Models.Account.LogoutViewModel

<h4>Logout</h4>
<hr/>
<p>Would you like to logout of IdentityServer?</p>
<form asp-action="Logout">
    <input type="hidden" name="logoutId" value="@Model.LogoutId"/>
    <fieldset>
        <div class="form-group">
            <button class="btn btn-outline-primary">Yes</button>
        </div>
    </fieldset>
</form>